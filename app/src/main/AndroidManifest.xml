<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mymapapplication.miguel.labrador.com.deadlifthelper" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label=""
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainScreen"
            android:label=""
            android:windowSoftInputMode="adjustPan" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RecordDeadlift"
            android:label="@string/title_activity_record_deadlift"
            android:parentActivityName=".MainScreen"
            android:windowSoftInputMode="adjustPan" >
>
        </activity>
        <activity
            android:name=".ViewLifts"
            android:label="@string/title_activity_view_lifts"
            android:parentActivityName=".MainScreen"
            android:windowSoftInputMode="adjustPan" >
        </activity>
        <activity
            android:name=".SetFilters"
            android:label="@string/title_activity_set_filters"
            android:parentActivityName=".ViewLifts"
            android:windowSoftInputMode="adjustPan" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="mymapapplication.miguel.labrador.com.deadlifthelper.ViewLifts" />
        </activity>
        <activity
            android:name=".TimerCount"
            android:label="@string/title_activity_timer_count"
            android:parentActivityName=".MainScreen" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="mymapapplication.miguel.labrador.com.deadlifthelper.MainScreen" />
        </activity>
    </application>

</manifest>
